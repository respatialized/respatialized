    [:span {:class "language-clojure keyword"} ":additive"]
    [:span {:class "language-clojure whitespace"} " "]
    [:span {:class "language-clojure string"} "&quot;sum&quot;"]
    [:span {:class "language-clojure comma"} [:span {:class "language-clojure comma"} ","]]
    [:span {:class "language-clojure whitespace"} " "]]
