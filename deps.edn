{:paths ["src" "resources" "content"]
 :deps
 {
  ;; core
  org.clojure/clojure {:mvn/version "1.10.3"}
  org.clojure/test.check {:mvn/version "1.1.0"}
  org.clojure/math.combinatorics {:mvn/version "0.1.6"}

  ;; site generation
  site.fabricate/fabricate
  {:git/url "https://github.com/fabricate-site/fabricate/"
   :sha "ba3c7545e7efb44a75f0b6ee38238d8b6546a436"}
  hiccup/hiccup {:mvn/version "2.0.0-alpha2" :exclusions [org.clojure/clojure]}
  hickory/hickory {:mvn/version "0.7.1"}
  garden/garden {:mvn/version "1.3.9"}

  ;; data munging
  org.clojure/data.xml {:mvn/version "0.2.0-alpha6"}
  metosin/malli #_{:mvn/version "0.8.4"}
  {:git/url "https://github.com/metosin/malli"
   :sha "6751b1d767f6189cde56f39c9e79e950b7264aca"}
  borkdude/sci {:mvn/version "0.2.6"}
  com.gfredericks/test.chuck {:mvn/version "0.2.10"}
  org.flatland/ordered {:mvn/version "1.5.9"}
  babashka/babashka.curl {:mvn/version "0.0.3"}

  ;; site content
  clojure2d/clojure2d {:mvn/version "1.4.3"}
  thi.ng/geom {:mvn/version "1.0.0-RC4"}
  org.apache.commons/commons-math3 {:mvn/version "3.6.1"}
  generateme/fastmath {:mvn/version "2.1.5"}
  scicloj/tablecloth {:mvn/version "6.076"}
  io.github.adam-james-v/svg-clj {:mvn/version "0.0.11"}
  applied-science/darkstar {:git/url "https://github.com/applied-science/darkstar/"
                            :sha "541a3ff36065c59e92fe6aa61e41a4385ba6f893"}

  ;; site indexing and dbs
  org.clojars.quoll/asami {:mvn/version "2.2.4"}


  ;; site building functions
  }
 :jvm-opts ["--illegal-access=warn"
            "-Dio.netty.tryReflectionSetAccessible=false"
            "-Xms4G" "-Xmx12G"]
 :aliases

 {:test {:extra-paths ["test"]
         :extra-deps {org.clojure/test.check {:mvn/version "1.1.0"}}}
  :runner
  {:extra-deps
   {io.github.cognitect-labs/test-runner
    {:git/tag "v0.5.0" :git/sha "b3fd0d2"}}
   :exec-fn cognitect.test-runner.api/test
   :main-opts ["-m" "cognitect.test-runner"]}
  :dev {:extra-paths ["test"]
        :extra-deps {criterium/criterium {:mvn/version "0.4.6"}
                     com.clojure-goes-fast/clj-async-profiler {:mvn/version "0.5.1"}
                     com.clojure-goes-fast/clj-memory-meter {:mvn/version "0.1.3"}
                     com.taoensso/tufte {:mvn/version "2.2.0"}
                     babashka/babashka.curl {:mvn/version "0.1.1"}}
        :jvm-opts ["-Djdk.attach.allowAttachSelf"
                   "-XX:+UnlockDiagnosticVMOptions"
                   "-XX:+DebugNonSafepoints"]}}}
